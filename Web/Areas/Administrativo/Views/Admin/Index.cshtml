
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@section breadcrumb{
    <li>
        <i class="ace-icon fa fa-home home-icon"></i>
        <a href="@Url.Action("Index","Home", null)">Home</a>
    </li>
    <li>
        <a href="@Url.Action("Index", "Publicacao")">Publicação</a>
    </li>
    <li class="active">Categoria</li>
}



<div class="page-content">
    <div class="page-header">
        <h1>
            Página administrativa
            <small>
                <i class="ace-icon fa fa-angle-double-right"></i>
                Listagem
            </small>
        </h1>
    </div><!-- /.page-header -->

    <div class="row">
        <div class="col-xs-12">
            <!-- PAGE CONTENT BEGINS -->

            <p>
                <button onclick="location.href='@Url.Action("ImovelUrl","Admin")'" class="btn btn-success btn-lg">
                    URL Imóveis
                </button>
            </p>
            <p>
                <button onclick="location.href='@Url.Action("BairroUrl","Admin")'" class="btn btn-success btn-lg">
                    URL Bairros
                </button>
            </p>
            <p>
                <button onclick="location.href='@Url.Action("CidadeUrl","Admin")'" class="btn btn-success btn-lg">
                    URL Cidades
                </button>
            </p>
            
            
        </div><!-- /.col -->
    </div><!-- /.row -->
</div><!-- /.page-content -->